[
  {
    "URL" : ["www.google.*/imgres", "www.google.*/images", "www.google.*/alerts", "www.google.*/custom"],
    "Proxy":["Direct", "Default"]
  },
  {
    "Host" : ["feedproxy.google.com", "picasa.google.com", "books.google.com"],
    "Proxy":["Google", "Default"]
  },
  {
    "Host" : ["*.ytimg.com*", "*.google.com(.*)?","*.googleusercontent.com*", "*.gstatic.com","*.googlecode.com"],
    "Proxy":["GoogleHttps", "Direct", "Default"]
  },
  {
    "Host" : ["*.doubleclick.net*", "*.google-analytics.com", "*.blogger.com", "*.google.co.*"],
    "Proxy":["GoogleHttps", "Direct", "Default"]
  },
  {
    "Host" : ["*.golang.org"],
    "Proxy": ["Direct", "Default"]
  },
  {
    "Host" : ["*.blogspot.*", "*.appspot.com", "golang.org"],
    "Proxy": ["GoogleHttps", "Default"]
  },
  {
    "Protocol" : "http",
    "Host": ["twitter.com", "(www.)?facebook.com"],
    "Attr":  ["RedirectHttps"]
  },
  {
    "Protocol" : "https",
    "Host": ["twitter.com"],
    "Proxy": ["Default"]
  }
]
